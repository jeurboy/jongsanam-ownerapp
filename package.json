{
  "name": "jongsanam-ownerapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "expo run:android",
    "ios": "react-native run-ios --simulator=\"iPad Pro 11-inch (M5)\"",
    "ios-device": "react-native run-ios --device",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "postinstall": "patch-package",
    "eas-build": "ENVFILE=.env.production eas build --platform ios",
    "build:android:release": "ENVFILE=.env.production cd android && ./gradlew assembleRelease && cd ..",
    "build:android:bundle": "ENVFILE=.env.production cd android && ./gradlew bundleRelease && cd ..",
    "build:ios:release": "ENVFILE=.env.production react-native run-ios --configuration Release --device",
    "build:ios:archive": "ENVFILE=.env.production cd ios && xcodebuild -workspace JongSanamOwnerApp.xcworkspace -scheme JongSanamOwnerApp -configuration Release -archivePath ./build/JongSanamOwnerApp.xcarchive archive && cd ..",
    "build:ios:export": "cd ios && xcodebuild -exportArchive -archivePath ./build/JongSanamOwnerApp.xcarchive -exportPath ./build -exportOptionsPlist exportOptions.plist && cd ..",
    "build:all": "npm run build:android:release && npm run build:ios:archive && npm run build:ios:export"
  },
  "dependencies": {
    "@expo/metro-config": "^54.0.12",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-clipboard/clipboard": "^1.16.3",
    "@react-native-firebase/analytics": "^23.7.0",
    "@react-native-firebase/app": "^23.7.0",
    "@react-native/new-app-screen": "0.83.1",
    "@react-navigation/native": "^7.1.26",
    "@react-navigation/native-stack": "^7.9.0",
    "date-fns": "^4.1.0",
    "expo": "~54.0.31",
    "expo-modules-core": "^3.0.29",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-bootsplash": "^6.3.11",
    "react-native-config": "^1.6.1",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-orientation-locker": "^1.7.0",
    "react-native-safe-area-context": "^5.5.2",
    "react-native-screens": "~4.16.0",
    "react-native-vector-icons": "^10.3.0",
    "react-native-vision-camera": "^4.7.3",
    "react-native-worklets-core": "^1.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "20.0.0",
    "@react-native-community/cli-platform-android": "20.0.0",
    "@react-native-community/cli-platform-ios": "20.0.0",
    "@react-native/babel-preset": "0.83.1",
    "@react-native/eslint-config": "0.83.1",
    "@react-native/metro-config": "0.83.1",
    "@react-native/typescript-config": "0.83.1",
    "@types/jest": "^29.5.13",
    "@types/react": "~19.1.10",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-test-renderer": "19.1.0",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "patch-package": "^8.0.1",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "2.8.8",
    "react-test-renderer": "19.1.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=20"
  }
}
